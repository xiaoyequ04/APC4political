# cbs <- read_csv("../data/cbs.csv",show_col_types = FALSE)
fulldata_cbs <- cbs %>%
left_join(.,mds_china,by = join_by(yearbirth == year)) %>%
group_by(cohort) %>%
mutate(gdppc_mean = mean(gdppc,na.rm = TRUE),
gdp_mean = mean(gdp,na.rm = TRUE),
growth_mean = mean(growth,na.rm = TRUE)) %>%
ungroup()
gdppc2autho_mean <- lm(autho ~ log(gdppc_mean)  + socialLevel +female + edu + hukouRural + interstpol + frepol + marriage + party, data = fulldata_cbs,weights = weight)
gpppc2ecofamaily5 <- update(gdppc2autho_mean, ecofamily5 ~ . )
mgdppc2autho_mean5 <- update(gdppc2autho_mean,.~ . + ecocountry5 + ecofamily5)
mgdppc2autho_mean <- update(gdppc2autho_mean,.~ . + ecocountry + ecofamily)
gdppc2autho <- update(gdppc2autho_mean, . ~ . - log(gdppc_mean) + log(gdppc))
gdp2autho_mean <- update(gdppc2autho_mean, . ~ . + log(gdp_mean) - log(gdppc_mean))
gdp2autho <- update(gdp2autho_mean, . ~ . - log(gdp_mean) + log(gdp))
gdp2ecofamily <- update(gdp2autho,ecofamily ~ .)
list_eco2author <- list(
gdppc2autho = gdppc2autho,
gdppc2autho_mean = gdppc2autho_mean,
mgdppc2autho_mean = mgdppc2autho_mean,
mgdppc2autho_mean5 = mgdppc2autho_mean5,
gdp2autho = gdp2autho,
gdp2autho_mean = gdp2autho_mean,
gdp2ecofamily = gdp2ecofamily
)
msummary(list_eco2author,
stars = TRUE)
